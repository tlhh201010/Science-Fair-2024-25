import math
longest = 0
current = 0
primes = []
long = []
currents = []
def checkPrime(number):
  divisors = []
  for i in range(1, math.ceil(math.sqrt(number))+1):
    if number / i == int(number / i):
      divisors.append(i)
      divisors.append(int(number / i))
  x = len(divisors)
  if len(divisors) == 2:
    return True
  else:
    return False
a = int(input('input an a'))
q = int(input('input a q'))
for i in range(10001):
  if checkPrime(i) == True:
    primes.append(i)
for i in range(1, len(primes) + 1):
  if primes[i-1] % q == a % q:
    current += 1
    currents.append(primes[i-1])
  else:
    current = 0
    currents = []
  if current >= longest:
    longest = current
    long = []
    long.append(currents)
print(longest)
print(long)
